<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
    <title>p5js Template</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.5.3/p5.js"></script>

    <script>
    
        var squarePressed;
        function setup() {
           
            createCanvas(600,800);
            background(90);
            squarePress = 0;red
    ellipse(100,100,100,100)    
        ellipse(100,300,100,100)   
     ellipse(500,100,100,100)  
ellipse(500,300,100,100)    
        }
    
        

        function draw() {
            noStroke();
          
            
            fill(255, 0, 0);
            rect(50, 400, 50, 50);

        
            fill(0, 255, 0)
            rect(150, 400, 50, 50);


            fill(0, 0, 255);
            rect(250, 400, 50, 50);

         

        }

        function mousePressed() {
            
            if (mouseX > 50 && mouseY > 400 && mouseX < 100 && mouseY < 450) {
                squarePressed = 1;
            }
            
            
            else if (mouseX > 150 && mouseY > 400 && mouseX < 200 && mouseY < 450) {
                squarePressed = 2;
            }
            
        
            else if (mouseX > 250 && mouseY > 400 && mouseX < 300 && mouseY < 450) {
                squarePressed = 3;
            }
            
           
           
            
        }

        function mouseDragged() {
            
            if (squarePressed == 1) {
                fill(255, 0, 0);
                noStroke();
                ellipse(mouseX, mouseY, 25, 25);
            }
            
            
            if (squarePressed == 2) {
                fill(0, 255, 0);
                noStroke();
                ellipse(mouseX, mouseY, 25, 25);
            }
            
            
            if (squarePressed == 3) {
                fill(0, 0, 255);
                noStroke();
                ellipse(mouseX, mouseY, 25, 25);
            }
        }
    </script>
</head>

<body>
    <div id="code" class="container-fluid">
        <script src="https://gist-it.appspot.com/github/christopherl4942/p5js/blob/master/project.html?footer=0"></script>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script>
        $(window).bind("load", function() {
            $('#code').before($('#defaultCanvas0').remove());
        });
    </script>
</body>

<!--puedocode:-->
<!--4 ellipses-->
<!--3 colors to choose from (RGB) -->
